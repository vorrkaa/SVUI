            <Layer level="BACKGROUND" textureSubLevel="1">
                <Texture parentKey="Skin" nonBlocking="true" file="Interface\AddOns\SVUI_!Core\assets\backgrounds\DEFAULT" setAllPoints="true">
                    <Color r="0.2" g="0.2" b="0.2" a="1" />
